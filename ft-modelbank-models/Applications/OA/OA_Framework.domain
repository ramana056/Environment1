        DEF_RECIPIENT_SEQ : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=I, maxLength=5, sysNumber=37.0, core=true, t24Name=DEF.RECIPIENT.SEQ, mandatory=false)
