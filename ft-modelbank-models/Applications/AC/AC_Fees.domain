            @t24:i (align=L, inputBehaviour=I, maxLength=3, sysNumber=4.0, core=true, t24Name=LAST.DAY.INCLUSIVE, mandatory=false)
        MOVEMENT_TYPE -> byValue AC_Fees:EB_ACCRUAL_PARAM__MOVEMENT_TYPE *
