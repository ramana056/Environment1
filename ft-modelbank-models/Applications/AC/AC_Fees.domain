        LINK_PL_CAT -> ST_Config:CATEGORY
            @i18n:Label (en="PL Category (Previous Year)")
            @i18n:Tooltip (en="PL Category ")
            @t24:i (align=R, inputBehaviour=I, maxLength=5, sysNumber=6.0, core=true, t24Name=LINK.PL.CAT, mandatory=false)
