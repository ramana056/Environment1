/* This table will be used to store details of the Action's
For Example, if the collector had any action on collection item's then that action details stored in this table by system and aslo customer response action's stored in the same table. */
    CL_ACTIVITY
        @i18n:Description (en="")
        @t24:i (t24Name=CL.ACTIVITY, level=C, type=L, allowedFunctions="L P S", localRefAllowed=false)
    {
/* This field hold the collector action's or customer action's code.
Following action made by collector
1. CALLO - collector call to customer
2. CALLI - customer call to collector
3. VISIT - Field visit */
        ACTION_CODE -> CL_Contract:CL_ACTION
            @i18n:Label (en=ID)
            @t24:i (align=L, inputBehaviour=I, maxLength=15, sysNumber=2.0, core=true, businessType=ANY, t24Name=ACTION.CODE, mandatory=false)
        /* specified date when the action is taken */
        ACTION_DATE : T24BusinessTypes:D
            @i18n:Label (en="Blocking Date")
            @t24:i (align=R, inputBehaviour=I, maxLength=8, sysNumber=10.0, core=true, t24Name=ACTION.DATE, mandatory=false)
        COLLECTOR : T24BusinessTypes:ANY
            @t24:i (align=L, inputBehaviour=I, maxLength=15, sysNumber=6.0, core=true, t24Name=COLLECTOR, mandatory=false)
        /* This field hold the customer No who is going to repay their overdue amount's. */
        CUSTOMER -> ST_Customer:CUSTOMER
            @i18n:Label (en=Customer)
            @i18n:Tooltip (en="Customer Liability")
            @t24:i (align=R, inputBehaviour=I, maxLength=10, sysNumber=1.0, core=true, businessType=CUS, t24Name=CUSTOMER, mandatory=false)
        /* This field hold the information about current queue when collector has taken action. */
        DEST_QUEUE -> CL_Contract:CL_QUEUE
            @t24:i (align=L, inputBehaviour=I, maxLength=15, sysNumber=5.0, core=true, businessType=ANY, t24Name=DEST.QUEUE, mandatory=false)
        /* Action End time */
        END_TIME : T24BusinessTypes:A
            @i18n:Label (en="End Time")
            @i18n:Tooltip (en="End Time")
            @t24:i (align=L, inputBehaviour=I, maxLength=5, sysNumber=12.0, core=true, t24Name=END.TIME, mandatory=false)
        KEY -> CL_Contract:CL_ACTIVITY PK required
            @i18n:Label (en=Key)
            @i18n:Tooltip (en="Indicates th transaction key")
            @t24:i (align=L, inputBehaviour=I, maxLength=30, sysNumber=0.0, core=true, businessType=ANY, t24Name=KEY, mandatory=true)
        NOTES -> byValue CL_Contract:CL_ACTIVITY__NOTES *
            @t24:i (t24Name=NOTES)
        /* This field hold the information about what commitment is gone between collector and customer. */
        OUTCOME_CODE -> CL_Contract:CL_OUTCOME
            @t24:i (align=L, inputBehaviour=I, maxLength=15, sysNumber=3.0, core=true, businessType=ANY, t24Name=OUTCOME.CODE, mandatory=false)
        /* Commitment/Repay Amount. */
        OUTCOME_DUE_AMT : T24BusinessTypes:AMT
            @t24:i (align=R, inputBehaviour=I, maxLength=15, sysNumber=8.0, core=true, t24Name=OUTCOME.DUE.AMT, mandatory=false)
        /* Commitment/Customer Response date */
        OUTCOME_DUE_DATE : T24BusinessTypes:D
            @t24:i (align=R, inputBehaviour=I, maxLength=8, sysNumber=7.0, core=true, t24Name=OUTCOME.DUE.DATE, mandatory=false)
        PAYMENT_REFERENCE : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=20, sysNumber=13.0, core=true, t24Name=PAYMENT.REFERENCE, mandatory=false)
        RESERVED_1 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=17.0, core=true, t24Name=RESERVED.1, mandatory=false)
        RESERVED_2 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=16.0, core=true, t24Name=RESERVED.2, mandatory=false)
        RESERVED_3 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=15.0, core=true, t24Name=RESERVED.3, mandatory=false)
        RESERVED_4 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=14.0, core=true, t24Name=RESERVED.4, mandatory=false)
        /* This field hold the information about Previous queue when collector has taken action. */
        SRC_QUEUE -> CL_Contract:CL_QUEUE
            @t24:i (align=L, inputBehaviour=I, maxLength=15, sysNumber=4.0, core=true, businessType=ANY, t24Name=SRC.QUEUE, mandatory=false)
        /* Action start time */
        START_TIME : T24BusinessTypes:A
            @i18n:Label (en="Start Time")
            @i18n:Tooltip (en="Start Time")
            @t24:i (align=L, inputBehaviour=I, maxLength=5, sysNumber=11.0, core=true, t24Name=START.TIME, mandatory=false)
    }
