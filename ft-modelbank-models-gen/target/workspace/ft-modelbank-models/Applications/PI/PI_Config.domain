    /* Payment Order parameter application describes the features which connects Payment Order and Payment System */
    PAYMENT_ORDER_PARAMETER
        @i18n:Description (en="")
        @t24:i (t24Name=PAYMENT.ORDER.PARAMETER, nonStop=true, level=I, type=H, allowedFunctions="A C D E I L P Q S", localRefApplicationSysNo=33.0, localRefAllowed=true)
