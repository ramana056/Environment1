        OriginalTransactionAmount : T24BusinessTypes:AMT
            @t24:i (align=R, inputBehaviour=I, maxLength=17, sysNumber=10.0, core=true, t24Name=OriginalTransactionAmount, typeModifiers="3", mandatory=false)
