    POR_CLG_IN_SETTLEMENT_PENDING__RejectedAmount
    {
/* multi value field associated with RejectedAmount
original transaction reference for which it was received reject of outward DD in a pacs.002
if empty, and RejectedAmount field contains a value, in this case the entire bulk was rejected */
        FTNumber : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=16, sysNumber=12.0, core=true, t24Name=FTNumber, mandatory=false)
/* multi value field associated with FTNumber
Keep peers of rejected amount along with corrisponding FTNumber ( original transaction reference).
If FTNumber field is empty, the entire bulk is rejected, and RejectedAmount field contains the GroupHeaderTotalInterbankSettlementAmount */
        RejectedAmount : T24BusinessTypes:AMT
            @t24:i (align=R, inputBehaviour=I, maxLength=17, sysNumber=11.0, core=true, t24Name=RejectedAmount, typeModifiers="3", mandatory=false)
    }
