        OriginalDebtorAcctOtherID : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=11, sysNumber=19.0, core=true, t24Name=OriginalDebtorAcctOtherID, mandatory=false)
