        CURR_NO : T24BusinessTypes:N
            @i18n:Label (en="Curr No")
            @t24:i (align=R, inputBehaviour=N, maxLength=6, sysNumber=31.0, core=true, t24Name=CURR.NO, mandatory=false)
        /* This field determines how many days after the requested collection date balance check should be retried to check if sufficient funds are available for a book direct debit. */
        CollectionRetryDays : T24BusinessTypes:A
