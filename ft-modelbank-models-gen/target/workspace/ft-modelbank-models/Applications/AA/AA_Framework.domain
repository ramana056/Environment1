        DATE_TIME -> byValue AA_Framework:AA_PARAMETER__DATE_TIME *
            @t24:i (t24Name=DATE.TIME)
        DEPT_CODE -> ST_Config:DEPT_ACCT_OFFICER
            @i18n:Label (en="Dept Code")
            @t24:i (align=R, inputBehaviour=N, maxLength=6, sysNumber=24.0, core=true, t24Name=DEPT.CODE, mandatory=false)
        EXCLUDE_GROUP -> byValue AA_Framework:AA_PARAMETER__EXCLUDE_GROUP *
            @t24:i (t24Name=EXCLUDE.GROUP)
        INPUTTER -> byValue AA_Framework:AA_PARAMETER__INPUTTER *
            @t24:i (t24Name=INPUTTER)
        INTEGRITY_CHECK_ROUTINE -> byValue AA_Framework:AA_PARAMETER__INTEGRITY_CHECK_ROUTINE *
            @t24:i (t24Name=INTEGRITY.CHECK.ROUTINE)
        OVERRIDE : T24BusinessTypes:N
            @i18n:Label (en=Override)
            @i18n:Tooltip (en="Overrides generated by this transaction")
            @t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=17.0, core=true, t24Name=OVERRIDE, mandatory=false)
/* IIndicates whether the settlement processing for settlement account needs to be reversed and replayed with the new or changed amounts/rates. Once we opted the Automatic behavior then the behavior should not be changed to Manual or Null. After setting Automatic at global level(System) then system will not allow to set values at company level.
Note:Backdated settlement functionality is enabled only if settlement account is AR/AC Account alone
Available options are:
NULL/MANUAL - System will behave as existing old settlement process.
AUTOMATIC - System will behave in new way where settlement processing is reversed and replayed. */
        RECONSTRUCT_SETTLEMENT : AA_Framework:AA_PARAMETER__RECONSTRUCT_SETTLEMENT
            @t24:i (align=L, inputBehaviour=I, maxLength=9, sysNumber=15.0, core=true, t24Name=RECONSTRUCT.SETTLEMENT, mandatory=false)
        RECORD_STATUS : T24BusinessTypes:STAT
            @i18n:Label (en="Record Status")
            @t24:i (align=L, inputBehaviour=N, maxLength=6, sysNumber=18.0, core=true, t24Name=RECORD.STATUS, mandatory=false)
        /* Indicates the number of activities that needs to be retained when the archiving is performed. Even though the user might want to archive when the activity count is greater than 50(ARCHIVE.ACTIVITIES field), he might want to retain the last 5 activities so that there is some residual content and it is not completely empty. Links to ARCHIVE.ACTIVITIES and cannot be greater or equal to that. */
        RETAIN_ACTIVITIES : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=I, maxLength=4, sysNumber=4.0, core=true, t24Name=RETAIN.ACTIVITIES, mandatory=false)
        /* Indicates the number of bills that needs to be retained when the archiving is performed. Even though the user might want to archive when the bill count is greater than 20(ARCHIVE.ACTIVITIES field), he might want to retain the last 3 bills so that there is some residual content and the file is not completely empty. Links to ARCHIVE.BILLS and cannot be greater or equal to that. */
        RETAIN_BILLS : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=I, maxLength=4, sysNumber=2.0, core=true, t24Name=RETAIN.BILLS, mandatory=false)
        RETAIN_SAME_DAY_CNT : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=I, maxLength=4, sysNumber=10.0, core=true, t24Name=RETAIN.SAME.DAY.CNT, mandatory=false)
