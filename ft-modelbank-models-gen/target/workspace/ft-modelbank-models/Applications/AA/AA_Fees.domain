            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=54.0, core=true, businessType=HOOKOTHER, t24Name=CHARGE.OVERRIDE.ROUTINE, typeModifiers="HOOK.AA.CHARGE.OVERRIDEAYES", mandatory=false)
