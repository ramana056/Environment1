        /* Specifies the date and time, when the record was last updated. */
        UPDATED_DATE_TIME : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=27.0, core=true, t24Name=UPDATED.DATE.TIME, mandatory=false)
