        MAPPING_LOG_REF -> byValue OA_Status:OA_APPLICATION_STATUS__MAPPING_LOG_REF *
            @t24:i (t24Name=MAPPING.LOG.REF)
        OWNER -> PW_Foundation:PW_PARTICIPANT
            @i18n:Label (en=Owner)
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=1.0, core=true, businessType=A, t24Name=OWNER, mandatory=false)
        /* This field will hold the PW.PROCESS record id which is linked to the application for the specific purpose. */
        PW_PROCESS_ID : T24BusinessTypes:A
            @i18n:Label (en="Process Id")
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=37.0, core=true, t24Name=PW.PROCESS.ID, mandatory=false)
        /* This field specifies the Current stage of the application. It could be used to identify the current stage of the application to initiate the next processing which can be PW activity. */
        STAGE : OA_Status:OA_APPLICATION_STATUS__STAGE
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=2.0, core=true, t24Name=STAGE, mandatory=false)
