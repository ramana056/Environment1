# UTF-8
/* Status Processing for Origination Architecture */
Domain OA_Status
    @t24:i (module=OA)
    @java:Package (value=com.temenos.t24.datamodel.oa.oa_status)
namespace "http://www.temenos.com/t24/OA_Status"
metamodelVersion 1.30.6
Classes {
/* OA will have a single status table OA.APPLICATION.STATUS and each record will be for a single purpose within an Application. Therefore, for a multipurpose application there will be a record for each purpose of the application.
The table will be comprised of both fields for the system defined statuses as well as a name-value pair of fields for the user defined status codes and their values. The record will only contain the user defined status specified for the purpose.
Fields STATUS.CODE and STATUS.VALUE would be used to store the user defined status. Remaining fields would be used to store the system defined statuses. */
    OA_APPLICATION_STATUS
        @i18n:Description (en="")
        @t24:i (t24Name=OA.APPLICATION.STATUS, level=I, type=L, allowedFunctions="L S", localRefAllowed=false)
    {
        ADVICE_TYPE -> byValue OA_Status:OA_APPLICATION_STATUS__ADVICE_TYPE *
            @t24:i (t24Name=ADVICE.TYPE)
        /* ID of an OA.APPLICATION */
        APPLICATION_ID -> OA_Status:OA_APPLICATION_STATUS PK required
            @i18n:Label (en="Application Id")
            @t24:i (align=L, inputBehaviour=I, maxLength=52, sysNumber=0.0, core=true, businessType=A, t24Name=APPLICATION.ID, mandatory=true)
        DATA_MODEL_CATEGORY -> byValue OA_Status:OA_APPLICATION_STATUS__DATA_MODEL_CATEGORY *
            @t24:i (t24Name=DATA.MODEL.CATEGORY)
        DECISION_TYPE -> byValue OA_Status:OA_APPLICATION_STATUS__DECISION_TYPE *
            @t24:i (t24Name=DECISION.TYPE)
        FORM_REFERENCE -> byValue OA_Status:OA_APPLICATION_STATUS__FORM_REFERENCE *
            @t24:i (t24Name=FORM.REFERENCE)
