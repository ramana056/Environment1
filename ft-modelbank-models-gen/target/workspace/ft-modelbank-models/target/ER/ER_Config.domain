        AVAILABLE_LIMIT : T24BusinessTypes:AMT
            @t24:i (align=R, inputBehaviour=N, maxLength=14, sysNumber=4.0, core=true, t24Name=AVAILABLE.LIMIT, typeModifiers="1", mandatory=false)
