/* This is the table that contains the details of individual limit breaches recorded at instances a breach was raised. Breaches could arise at 3 instances.

1. When intraday or overnight breaches are encountered - During deal input(intraday) and COB(overnight)
2. When TY.DEALER.TXN.LIMIT is breached. - During deal input
3. When stop loss limit is breached. - By a stop loss service */
    TY_LIMIT_REPORT
        @i18n:Description (en="")
        @t24:i (t24Name=TY.LIMIT.REPORT, level=F, type=L, allowedFunctions="L P S", localRefApplicationSysNo=23.0, localRefAllowed=true)
    {
        /* This field contains the currency in which a breach is raised. */
        CURRENCY -> ST_CurrencyConfig:CURRENCY
            @i18n:Label (en=Ccy)
            @t24:i (align=L, inputBehaviour=I, maxLength=3, sysNumber=1.0, core=true, businessType=CCY, t24Name=CURRENCY, mandatory=false)
        /* This field contains the dealer desk that input a transaction that has led to a limit breach. */
        DEALER_DESK : T24BusinessTypes:A
            @i18n:Label (en="Dealer Desk")
            @i18n:Tooltip (en="Specifies the Dealer Desk handling the transaction")
            @t24:i (align=L, inputBehaviour=I, maxLength=19, sysNumber=6.0, core=true, t24Name=DEALER.DESK, mandatory=false)
        /* This field contains any additional notes entered by the dealer at the time of transaction input when a breach was encountered. */
        DEALER_NOTES : T24BusinessTypes:A
            @i18n:Label (en="Dealer Instr")
            @t24:i (align=L, inputBehaviour=I, maxLength=19, sysNumber=7.0, core=true, t24Name=DEALER.NOTES, mandatory=false)
        /* This field contains the transaction amount when a breach was raised. */
        DEAL_AMT : T24BusinessTypes:AMT
            @i18n:Label (en="Deal Amount")
            @t24:i (align=R, inputBehaviour=I, maxLength=19, sysNumber=11.0, core=true, t24Name=DEAL.AMT, mandatory=false)
        /* This field contains the date on which the transaction was input and a breach was raised. */
        DEAL_DATE : T24BusinessTypes:D
            @i18n:Label (en="Deal Date")
            @i18n:Tooltip (en="Indicates the Deal date")
            @t24:i (align=R, inputBehaviour=I, maxLength=8, sysNumber=10.0, core=true, t24Name=DEAL.DATE, mandatory=false)
        /* This field contain the transaction reference during the input of which a breach was encountered. */
        DEAL_REF : T24BusinessTypes:A
            @i18n:Label (en="Transaction Ref")
            @t24:i (align=L, inputBehaviour=I, maxLength=19, sysNumber=9.0, core=true, t24Name=DEAL.REF, mandatory=false)
        /* This field contains the amount by which the current transaction is breached. */
        EXCEEDED_BY : T24BusinessTypes:AMT
            @t24:i (align=R, inputBehaviour=I, maxLength=19, sysNumber=5.0, core=true, t24Name=EXCEEDED.BY, mandatory=false)
        /* This field contains the time at which the limit breach was encountered. */
        LIMIT_BREACH_TIME : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=19, sysNumber=8.0, core=true, t24Name=LIMIT.BREACH.TIME, mandatory=false)
        /* This field contains the limit amount that is breached by an incoming transaction. */
        LIMIT_EXPOSURE : T24BusinessTypes:AMT
            @t24:i (align=R, inputBehaviour=I, maxLength=19, sysNumber=2.0, core=true, t24Name=LIMIT.EXPOSURE, mandatory=false)
        /* This field contains the limit utilization amount after a breach is encountered. */
        LIMIT_UTIL_AFTER : T24BusinessTypes:AMT
