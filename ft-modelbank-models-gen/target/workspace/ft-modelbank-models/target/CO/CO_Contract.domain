        REGISTER_DATE : T24BusinessTypes:D
            @t24:i (align=R, inputBehaviour=I, maxLength=8, sysNumber=32.26, core=false, t24Name=REGISTER.DATE, mandatory=false)
        REGISTRATION_NO : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=32.3, core=false, t24Name=REGISTRATION.NO, mandatory=false)
        RESERVED1 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=0, sysNumber=30.0, core=true, t24Name=RESERVED1, mandatory=false)
        RESERVED13 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=0, sysNumber=6.0, core=true, t24Name=RESERVED13, mandatory=false)
        RESERVED14 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=0, sysNumber=5.0, core=true, t24Name=RESERVED14, mandatory=false)
        RESERVED2 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=0, sysNumber=29.0, core=true, t24Name=RESERVED2, mandatory=false)
        RESERVED3 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=0, sysNumber=28.0, core=true, t24Name=RESERVED3, mandatory=false)
        RESERVED4 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=0, sysNumber=27.0, core=true, t24Name=RESERVED4, mandatory=false)
        RESERVED5 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=0, sysNumber=26.0, core=true, t24Name=RESERVED5, mandatory=false)
        REVALUED_DATE -> byValue CO_Contract:COLLATERAL__REVALUED_DATE *
            @t24:i (t24Name=REVALUED.DATE)
/* The date and frequency of review of the collateral.
A combined date & frequency field which is automatically cycled by the system as part of the collateral application's end-of-day.
Validation Rules:
17 type FQU (combined date & frequency code).  (Optional input) */
        REVIEW_DATE_FQU : T24BusinessTypes:FQU
            @i18n:Label (en="Review Frequency")
            @i18n:Tooltip (en="Date and frequency for review of the collateral")
            @t24:i (align=L, inputBehaviour=I, maxLength=17, sysNumber=16.0, core=true, t24Name=REVIEW.DATE.FQU, mask="RDD DDD DDDD #####", mandatory=false)
        SECURITY_CODE : T24BusinessTypes:A
            @i18n:Label (en="Security No")
            @i18n:Tooltip (en="Indicates a valid Security")
            @t24:i (align=L, inputBehaviour=N, maxLength=12, core=false, t24Name=SECURITY.CODE, mandatory=false)
/* The status of the collateral object.
The status of the collateral object is indicated in this field as follows:
FWD - forward status; validity date of the corresponding right is forward;
CUR - current status; expiry date of the corresponding right is forward &       validity date is not;
LIQ - liquidated status; expiry date of either the corresponding right or       the object itself is past;
MAT - matured status; record occurs only on history file.
EXC - Excluded status; collateral is marked for exclusion i.e. the EXCLUSION.ID field is not null
In most cases, the status indicated against the collateral object will be the same as that belonging to the corresponding right. Exceptions are when a collateral object has an earlier expiry date than its corresponding right (in which case the object is considered expired whilst the right remains current) and when a collateral is marked for exclusion
This field is automatically maintained by the system by an end-of-day process and is updated in accordance with any changes to the dates and exclusion id defined on-line.
Validation Rules:
'FWD', 'CUR', 'LIQ', 'MAT' or 'EXC'.  Internal field.  This is a NOINPUT field. */
        STATUS : T24BusinessTypes:A
            @i18n:Label (en=Status)
            @t24:i (align=L, inputBehaviour=N, maxLength=3, sysNumber=34.0, core=true, t24Name=STATUS, mandatory=false)
/* Optional field, this field will be used to capture the status of the good, possible values are Finalised, Mortgage Liability and Foreclosed Asset
Cars/Vehicles */
        STATUS_GOOD : CO_Contract:COLLATERAL__STATUS_GOOD
            @t24:i (align=L, inputBehaviour=I, maxLength=15, sysNumber=32.10, core=false, t24Name=STATUS.GOOD, mandatory=false)
/* Optional field, this field will be used to capture the details of the tenant, this is to specify if the building
is occupied by tenant, will have three options yes, no or none */
        TENANT : CO_Contract:COLLATERAL__TENANT
            @t24:i (align=L, inputBehaviour=I, maxLength=10, sysNumber=32.12, core=false, t24Name=TENANT, mandatory=false)
/* A value which represents a prior or third party right to the collateral.
One of five value fields attached to the collateral.
This value is a value attached to the collateral which is not available under the right itself. It usually represents a third party value in cases such as, for example, a second mortgage.
The value in this field is subtracted from the EXECUTION.VALUE in order to determine the net execution value.
The allowable form of input to this field is determined by the field of the same name on the collateral type file.
The third party value may be defined on this table as mandatory, optional or non-input.
Allows positive, negative or both balances depending on ALLOWED.ACT.BAL set in COLLATERAL.TYPE
Validation Rules:
Up to 19-digit numeric, inclusive decimal point (amount format).  (Optional input, depending on collateral type).
No input is allowed if the collateral type record contains the definition 'N' in THIRD.PARTY.VALUE.
Input is mandatory if the collateral type record contains the definition 'M' in THIRD.PARTY.VALUE.
An override is required if the value exceeds the EXECUTION.VALUE. */
        THIRD_PARTY_VALUE : T24BusinessTypes:AMT
            @i18n:Label (en="Third Party Value")
            @t24:i (align=R, inputBehaviour=I, maxLength=19, sysNumber=12.0, core=true, t24Name=THIRD.PARTY.VALUE, typeModifiers="-7", mandatory=false)
        /* Optional field, this field will be used to capture the type of building, possible values are home, office and mixed */
        TYPE_BUILDING : CO_Contract:COLLATERAL__TYPE_BUILDING
            @t24:i (align=L, inputBehaviour=I, maxLength=40, sysNumber=32.21, core=false, t24Name=TYPE.BUILDING, mandatory=false)
        /* Optional field, this field will be used to capture the area of the building which is available for usage . Numeric */
        USEFUL_AREA : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=I, maxLength=8, sysNumber=32.16, core=false, t24Name=USEFUL.AREA, mandatory=false)
        US_STATE -> NACUST_Foundation:US_STATE
            @t24:i (align=L, inputBehaviour=I, maxLength=2, sysNumber=32.34, core=false, businessType=A, t24Name=US.STATE, mandatory=false)
/* The date upon which the values in the preceding fields were established or accepted.
The date on which the collateral values were accepted and/or recorded in the system.
The date may be used for local requirements in connection with the management of collateral. It is not the subject of any further processing and is not updated by the system when a revaluation is performed.
Validation Rules:
11 type D (date format).  (Mandatory input, default value is today's date). */
        VALUE_DATE : T24BusinessTypes:D
            @i18n:Label (en="Value Date")
            @i18n:Tooltip (en="Indicates the Value date of the Contract")
            @t24:i (align=R, inputBehaviour=I, maxLength=11, sysNumber=15.0, core=true, t24Name=VALUE.DATE, mandatory=false)
        VEHICLE_MODEL : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=32.1, core=false, t24Name=VEHICLE.MODEL, mandatory=false)
        YEAR_CONSTRUCT : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=I, maxLength=4, sysNumber=32.15, core=false, t24Name=YEAR.CONSTRUCT, mandatory=false)
    }
/* The collateral right file is used to record the status of a right of security and to indicate against which customer outstandings the right applies.
The link between the collateral right and the collateral object itself is implicit in the item-id structure between the two files.

LIMIT REFERENCE
The limit reference field on the collateral right record is used to identify which contract(s) the collateral is valid against.
This link between the collateral and the outstanding debit balances belonging to the customer may be made at one of three levels as follows:
contract/account level: the collateral is valid only against a given contract or set of contracts.
limit level: the collateral is valid against all contracts currently recorded under a particular limit reference and against any contracts which subsequently come under that limit.
customer level: the collateral is valid against any debit balance which exists or arises on the customer's accounts or contracts with the bank.
The latter case is implied by leaving the limit reference field blank on the collateral right record. Otherwise, the appropriate limits or contract references should be entered.
It is important to note that, if collateral values are being used in a process of reallocating debit balances across general ledger (or central bank) reporting lines, the level at which this link is made affects the priority with which debit balances are considered for reallocation. If a mixture of these levels is in use, all contract/account level balances will be considered first by the system, followed by limit-level balances and finally, any remaining balances attributable at customer-level will be considered. */
    COLLATERAL_RIGHT
        @i18n:Description (en="")
        @t24:i (t24Name=COLLATERAL.RIGHT, nonStop=true, level=C, type=H, allowedFunctions="A C D E H I L P Q R S", localRefApplicationSysNo=18.0, localRefAllowed=true)
    {
/* This is a work field used by the system to maintain a temporary pointer to a work record used in the allocation of collateral. It has no direct business use.
Validation Rules:

System-maintained */
        ALLOC_WORK_ID : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=N, maxLength=12, sysNumber=22.0, core=true, t24Name=ALLOC.WORK.ID, mandatory=false)
        AUDITOR_CODE : T24BusinessTypes:A
            @i18n:Label (en="Auditor Code")
            @i18n:Tooltip (en="Auditor Code")
            @t24:i (align=L, inputBehaviour=E, maxLength=16, sysNumber=34.0, core=true, t24Name=AUDITOR.CODE, mandatory=false)
        AUDIT_DATE_TIME : T24BusinessTypes:RELTIME
            @i18n:Label (en="Audit Date Time")
            @i18n:Tooltip (en="Date and time when record was audited")
            @t24:i (align=L, inputBehaviour=E, maxLength=15, sysNumber=35.0, core=true, t24Name=AUDIT.DATE.TIME, mask="RDD DD  DD ##:##", mandatory=false)
        AUTHORISER : T24BusinessTypes:A
            @i18n:Label (en=Authoriser)
            @i18n:Tooltip (en="Indicates the User who Authorises the record")
            @t24:i (align=L, inputBehaviour=N, maxLength=100, sysNumber=31.0, core=true, t24Name=AUTHORISER, mandatory=false)
/* Code of the category to which the collateral right belongs.
The content of this field must be a valid code defined on the collateral code file.
Validation Rules:
1-3 digit numeric. (Mandatory input) */
        COLLATERAL_CODE -> CO_Config:COLLATERAL_CODE required
            @i18n:Label (en="Collateral Code")
            @i18n:Tooltip (en="Indicates the Collateral attached to the Limit")
            @t24:i (align=R, inputBehaviour=I, maxLength=3, sysNumber=1.0, core=true, t24Name=COLLATERAL.CODE, mandatory=true)
/* Unique identification of collateral right. This item-id consists of the customer id followed by a sequence number. The two are separated by a period (full stop).
The item-id by which the collateral right is identified.
The item-id to the collateral right file is made up of two components, separated by a period (full stop).
The first component is the customer id. This is the customer against whose outstandings the collateral is held. All subsequently mentioned accounts, limits or transaction references must belong to this customer (exception=override condition).
This first component must be a valid customer id defined on the customer file.
The second component identifies, where required, further rights of security given by the customer. The default value of this sequence number is one.
When the customer number has been entered by the user, the system will automatically append the default sequence number of '.1'. The user may subsequently enter only the second (suffix) part of the item-id to bring up further records belonging to that customer.
This item-id also forms the first two components of the item-id to records on the collateral main file.
Validation Rules:
1-10 digit numeric (customer id), period, 1 to 4-digit numeric (sequence number).
The sequence number following the full stop may not be zero. */
        COLLATERAL_RIGHT -> CO_Contract:COLLATERAL_RIGHT PK required
            @i18n:Label (en="Collateral Right")
            @t24:i (align=L, inputBehaviour=I, maxLength=18, sysNumber=0.0, core=true, businessType=A, t24Name=COLLATERAL.RIGHT, mandatory=true)
        COMPANY -> byValue CO_Contract:COLLATERAL_RIGHT__COMPANY *
            @t24:i (t24Name=COMPANY)
        CO_CODE -> ST_CompanyCreation:COMPANY
            @i18n:Label (en=Company)
            @t24:i (align=L, inputBehaviour=N, maxLength=11, sysNumber=32.0, core=true, businessType=A, t24Name=CO.CODE, mask="R##-###-####", mandatory=false)
        CURR_NO : T24BusinessTypes:N
            @i18n:Label (en="Curr No")
            @t24:i (align=R, inputBehaviour=N, maxLength=6, sysNumber=28.0, core=true, t24Name=CURR.NO, mandatory=false)
        CUSTOMER -> byValue CO_Contract:COLLATERAL_RIGHT__CUSTOMER *
            @t24:i (t24Name=CUSTOMER)
        CUSTOMER_ID : T24BusinessTypes:N
            @i18n:Label (en="Customer Id")
            @i18n:Tooltip (en="Identifies the Customer for the transaction.")
            @t24:i (align=L, inputBehaviour=N, maxLength=19, core=false, t24Name=CUSTOMER.ID, genOperation="FIELD(@ID,\'.\',1)", mandatory=false)
        DATE_TIME -> byValue CO_Contract:COLLATERAL_RIGHT__DATE_TIME *
            @t24:i (t24Name=DATE.TIME)
        DEPT_CODE -> ST_Config:DEPT_ACCT_OFFICER
            @i18n:Label (en="Dept Code")
            @t24:i (align=R, inputBehaviour=N, maxLength=6, sysNumber=33.0, core=true, t24Name=DEPT.CODE, mandatory=false)
/* The expiry date of the collateral right.
For certain types of collateral, the values attached to the collateral may only be valid for a limited period. This field may be used to record, in advance, the expiry date associated with the collateral.
Although this is mainly for reporting and management purposes, the presence of a date in this field also blocks the reallocation process for the collateral values (after such date has passed).
A past date in this field always indicates a status of 'liquidated', both on the collateral right itself and on any associated collateral objects.
Validation Rules:
11 type D (date format).  (Optional input)
The date may not be earlier than the VALIDITY.DATE. */
        EXPIRY_DATE : T24BusinessTypes:D
            @i18n:Label (en="Expiry Date")
            @i18n:Tooltip (en="Indicates the Expiry Date of the Transaction")
            @t24:i (align=R, inputBehaviour=I, maxLength=11, sysNumber=14.0, core=true, t24Name=EXPIRY.DATE, mandatory=false)
        INPUTTER -> byValue CO_Contract:COLLATERAL_RIGHT__INPUTTER *
            @t24:i (t24Name=INPUTTER)
        LIMIT_ID -> byValue CO_Contract:COLLATERAL_RIGHT__LIMIT_ID *
            @t24:i (t24Name=LIMIT.ID)
/* A customer id which may be used to reference details of the public notary.
Details of the public notary are recorded on a customer record, which is indicated in this field.
Validation Rules:
1-10 digit numeric (customer id).  (Optional input) */
        NOTARY -> ST_Customer:CUSTOMER
            @t24:i (align=R, inputBehaviour=I, maxLength=10, sysNumber=15.0, core=true, businessType=CUS, t24Name=NOTARY, mandatory=false)
        NOTES -> byValue CO_Contract:COLLATERAL_RIGHT__NOTES *
            @t24:i (t24Name=NOTES)
        /* Reserved for future use. */
        OS_PERCENT_COVER : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=3, sysNumber=21.0, core=true, t24Name=OS.PERCENT.COVER, mandatory=false)
        OVERRIDE -> byValue CO_Contract:COLLATERAL_RIGHT__OVERRIDE *
            @t24:i (t24Name=OVERRIDE)
/* The percentage of cover which applies to the collateral.
The percentage of cover belonging to a collateral right is calculated by the formula:
% of cover = 100 x debit balance / value
where:
debit balance = (sum of) debit balance(s) under the limit reference (or                 contract id) belonging to the collateral right.
value = (sum of) net execution value(s) belonging to collateral objects         attached to the right,
net execution value = execution value - third party value.
Note that, for the purposes of this calculation, the system always assumes that the entire value of a given entity belongs exclusively to the right under which it occurs.
The percentage of cover may initially be entered by the user and subsequently updated periodically by the system according to a frequency determined per collateral code.
This field may take the value zero (=0%) or null (=unknown or not applicable) or greater than 100%.
The system-calculated value is available as an on-line default (when attached collateral values have been authorized), but will not override any user-input.
The value in this field is for information only, and has no subsequent impact on the system's reallocation process (ie. in determining whether a given level of excess value exists).
Note : If the system's calculation of this value yields an invalid result,        the value 999 will be returned.
Validation Rules:
1 to 3-digit numeric. (Optional input) */
        PERCENTAGE_COVER : T24BusinessTypes:N
            @i18n:Label (en="% Cover")
            @t24:i (align=R, inputBehaviour=I, maxLength=3, sysNumber=11.0, core=true, t24Name=PERCENTAGE.COVER, mandatory=false)
        RECORD_STATUS : T24BusinessTypes:STAT
            @i18n:Label (en="Record Status")
            @t24:i (align=L, inputBehaviour=N, maxLength=6, sysNumber=27.0, core=true, t24Name=RECORD.STATUS, mandatory=false)
        RESERVED_1 : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=N, maxLength=32, sysNumber=25.0, core=true, t24Name=RESERVED.1, mandatory=false)
        RESERVED_2 : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=N, maxLength=32, sysNumber=24.0, core=true, t24Name=RESERVED.2, mandatory=false)
        RESERVED_3 : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=N, maxLength=32, sysNumber=23.0, core=true, t24Name=RESERVED.3, mandatory=false)
/* The review date & frequency which is to apply to the collateral right.
The date of the next review combined with the frequency of review.
A default frequency may be defined on the collateral code file. If so, the value of this field defaults to one cycle forward from today's date according to this default frequency.
The date part of this field is automatically cycled by the system.
Validation Rules:
17 type frequency format.  (Optional input; default, if applicable, according to the collateral code) */
        REVIEW_DATE_FQU : T24BusinessTypes:FQU
            @i18n:Label (en="Review Frequency")
            @i18n:Tooltip (en="Indicates the review date and frequency")
            @t24:i (align=L, inputBehaviour=I, maxLength=17, sysNumber=13.0, core=true, t24Name=REVIEW.DATE.FQU, mask="RDD DDD DDDD #####", mandatory=false)
/* The status of the collateral right.
The status of the collateral right is indicated in this field as follows:
FWD - forward status; validity date is forward;
CUR - current status; expiry date is forward & validity date is not;
LIQ - liquidated status; expiry date is past;
MAT - matured status; record occurs only on history file.
The only status under which a collateral right is considered by the system's reallocation process is current.
In most cases, the status indicated against the collateral right will also be indicated against any collateral objects belonging to the right. The sole exception is when a collateral object has an earlier expiry date than its corresponding right (in which case the object is considered expired whilst the right remains current).
This field is automatically maintained by the system by an end-of-day process and is updated in accordance with any changes to the dates entered on-line.
Validation Rules:
'FWD', 'CUR', 'LIQ' or 'MAT'.
Internal field.  This is a NOINPUT field. */
        STATUS : CO_Contract:COLLATERAL_RIGHT__STATUS
            @i18n:Label (en=Status)
            @t24:i (align=L, inputBehaviour=N, maxLength=3, sysNumber=19.0, core=true, t24Name=STATUS, mandatory=false)
        VALIDITY_CHK : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=10, core=false, t24Name=VALIDITY.CHK, genOperation="SUBR(\"MB.SS.VALIDITY.CHK\",@ID)", mandatory=false)
/* The date upon which the collateral right becomes effective.
This date corresponds to the date on which the right of security becomes legal.
A forward date may be entered, in which case no reallocation of the attached collateral values will take place (the status of the record is treated as 'forward').
Validation Rules:
11 type D characters.
Mandatory input; default is today's date. */
        VALIDITY_DATE : T24BusinessTypes:D
            @i18n:Label (en="Valid Date")
            @i18n:Tooltip (en="Indicates the date from which the link is valid")
            @t24:i (align=R, inputBehaviour=I, maxLength=11, sysNumber=12.0, core=true, t24Name=VALIDITY.DATE, mandatory=false)
    }
    /* This file holds the list of Collateral Right Ids and the corresponding Limit References for a Customer. The Id of the table will be a Customer Id. This table will be updated when a Collateral Right is authorised. When a multi-customer limit is attached to the Collateral Right, this table will be updated for each customer specified in the limit record */
    COLLATERAL_RIGHT_CUST
        @i18n:Description (en="")
        @t24:i (t24Name=COLLATERAL.RIGHT.CUST, level=C, type=T, allowedFunctions="L P S", localRefAllowed=false)
    {
        COLL_RIGHT_ID -> byValue CO_Contract:COLLATERAL_RIGHT_CUST__COLL_RIGHT_ID *
            @t24:i (t24Name=COLL.RIGHT.ID)
