        /* Validation Rules:Reserved fields for Account with bank */
        ACCT_WITH_BANK_RESERVED_3 : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=48.0, core=true, t24Name=ACCT.WITH.BANK.RESERVED.3, mandatory=false)
