        BENEFICIARY_OT_ID_TYPE : PI_Contract:PAYMENT_ORDER__BENEFICIARY_OT_ID_TYPE
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=37.0, core=true, t24Name=BENEFICIARY.OT.ID.TYPE, mandatory=false)
