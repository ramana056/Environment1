        BENEFICIARY_BR_PRVNC : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=34.0, core=true, t24Name=BENEFICIARY.BR.PRVNC, mandatory=false)
