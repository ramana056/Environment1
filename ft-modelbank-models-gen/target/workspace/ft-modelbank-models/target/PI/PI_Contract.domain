        CHEQUE_NUMBER : T24BusinessTypes:A
            @i18n:Label (en="Cheque No")
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=184.0, core=true, t24Name=CHEQUE.NUMBER, mandatory=false)
/* Clearing channel for the order
