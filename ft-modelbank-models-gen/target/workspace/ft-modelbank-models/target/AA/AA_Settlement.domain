            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=3.0, core=true, businessType=HOOKOTHER, t24Name=USER.RULE.ROUTINE, typeModifiers="HOOK.AA.SETTLEMENT.TYPEA", mandatory=false)
    }
