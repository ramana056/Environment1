        PROJ_ESCROW_BAL : T24BusinessTypes:BMT
            @t24:i (align=R, inputBehaviour=I, maxLength=25, sysNumber=16.0, core=true, t24Name=PROJ.ESCROW.BAL, mandatory=false)
