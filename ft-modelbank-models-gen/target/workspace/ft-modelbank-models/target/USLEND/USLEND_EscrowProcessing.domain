    /* This table is used to store Escrow analysis projections per arrangement. This table is updated by the system either upon triggering LENDING-NEW-ARRANGEMENT or LENDING-CHANGE-ESCROW activity when ANALYSIS field in ESCROW charge property is set to YES or NXT.ANLYSIS.FQU field in PAYMENT.SCHEDULE property class is TODAY. */
    USLEND_ESCROW_ANALYSIS_DETS
        @i18n:Description (en="")
        @t24:i (t24Name=USLEND.ESCROW.ANALYSIS.DETS, level=F, type=L, allowedFunctions="L P S", localRefApplicationSysNo=37.0, localRefAllowed=true)
    {
/* Identifies the type of analysis done either INITIAL or ACTUAL
Validation Rules

INTIAL - Analysis done upon enrolment for Escrow processing, i.e. when customer opts of escrow processing on the arrangement either on a NEW/existing arrangement.
ACTUAL - Analysis done by the system based on NXT.ANLYSIS.FQU field in PAYMENT.SCHEDULE property class */
        ANALYSIS_TYPE : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=19, sysNumber=1.0, core=true, t24Name=ANALYSIS.TYPE, mandatory=false)
        ANNUAL_ANALYSIS_DT : T24BusinessTypes:D
            @t24:i (align=R, inputBehaviour=I, maxLength=12, sysNumber=23.0, core=true, t24Name=ANNUAL.ANALYSIS.DT, mandatory=false)
        /* Stores the date on which the analysis was performed */
        CALC_DATE : T24BusinessTypes:D
            @i18n:Label (en="Calc Date")
            @t24:i (align=R, inputBehaviour=I, maxLength=12, sysNumber=2.0, core=true, t24Name=CALC.DATE, mandatory=false)
        CUSHION_AMOUNT : T24BusinessTypes:AMT
            @t24:i (align=R, inputBehaviour=I, maxLength=35, sysNumber=10.0, core=true, t24Name=CUSHION.AMOUNT, mandatory=false)
        DATE -> byValue USLEND_EscrowProcessing:USLEND_ESCROW_ANALYSIS_DETS__DATE *
            @t24:i (t24Name=DATE)
        /* The ID comprises of Arrangement ID - Date */
        ESCROW_ANALYSIS -> USLEND_EscrowProcessing:USLEND_ESCROW_ANALYSIS_DETS PK required
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=0.0, core=true, businessType=A, t24Name=ESCROW.ANALYSIS, mandatory=true)
        ESCROW_BALANCE : T24BusinessTypes:AMT
            @t24:i (align=R, inputBehaviour=I, maxLength=35, sysNumber=9.0, core=true, t24Name=ESCROW.BALANCE, mandatory=false)
        ESCROW_PAYMENT : T24BusinessTypes:BMT
            @t24:i (align=R, inputBehaviour=I, maxLength=25, sysNumber=25.0, core=true, t24Name=ESCROW.PAYMENT, mandatory=false)
/* This field contains the initial amount required from the customer once the analysis is performed. The initial amount is calculated taking into consideration the available balance in ESCROWBAL balance type.

Validation rules

1-16 AMT Standard amount format. */
        INITIAL_AMOUNT : T24BusinessTypes:AMT
            @t24:i (align=R, inputBehaviour=I, maxLength=35, sysNumber=7.0, core=true, t24Name=INITIAL.AMOUNT, mandatory=false)
        INIT_STMT_STATUS : T24BusinessTypes:A
            @t24:i (align=L, inputBehaviour=I, maxLength=10, sysNumber=26.0, core=true, t24Name=INIT.STMT.STATUS, mandatory=false)
        MINIMUM_BAL : T24BusinessTypes:BMT
            @i18n:Label (en="Minimum Bal")
            @i18n:Tooltip (en="Indicates the minimum balance to be maintaintained")
            @t24:i (align=R, inputBehaviour=I, maxLength=35, sysNumber=18.0, core=true, t24Name=MINIMUM.BAL, mandatory=false)
        NEW_ESCROW_AMOUNT : T24BusinessTypes:AMT
            @t24:i (align=R, inputBehaviour=I, maxLength=35, sysNumber=8.0, core=true, t24Name=NEW.ESCROW.AMOUNT, mandatory=false)
        NEW_ESCROW_AMT_EFF : T24BusinessTypes:D
            @t24:i (align=R, inputBehaviour=I, maxLength=12, sysNumber=15.0, core=true, t24Name=NEW.ESCROW.AMT.EFF, mandatory=false)
        PREVIOUS_CALC_DATE : T24BusinessTypes:D
            @t24:i (align=R, inputBehaviour=I, maxLength=12, sysNumber=21.0, core=true, t24Name=PREVIOUS.CALC.DATE, mandatory=false)
        PREV_ANNUAL_DT : T24BusinessTypes:D
