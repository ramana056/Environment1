    USLEND_ESCROW_ANALYSIS_DETS__DATE
    {
/* This field contains the disbursement amount(amount prefixed with a hyphen '-') and escrow instalment amount for the corresponding DATE field.

Validation rules

Positive amount signifies escrow instalment amount
Negative amount signifies escrow disbursement amount */
        AMOUNT : T24BusinessTypes:BMT
            @i18n:Label (en=Amount)
            @i18n:Tooltip (en="Indicates the Amount")
            @t24:i (align=R, inputBehaviour=I, maxLength=20, sysNumber=5.0, core=true, t24Name=AMOUNT, mandatory=false)
/* Amount in this field is the summation of INITIAL.AMOUNT + AMOUNT for each given DATE.

Validation rules

1-16 AMT Standard amount format. */
        BALANCE : T24BusinessTypes:BMT
            @i18n:Label (en=Balance)
            @i18n:Tooltip (en="Indicates the current Balance of the Product")
            @t24:i (align=R, inputBehaviour=I, maxLength=20, sysNumber=6.0, core=true, t24Name=BALANCE, mandatory=false)
/* Escrow payment schedule dates and Escrow disbursement dates for each payee

Validation rules

Standard T24 DATE format, i.e. YYYYMMDD */
        DATE : T24BusinessTypes:D
            @i18n:Label (en=Date)
            @t24:i (align=R, inputBehaviour=I, maxLength=12, sysNumber=3.0, core=true, t24Name=DATE, mandatory=false)
        PAYMENT_DESC : T24BusinessTypes:ANY
