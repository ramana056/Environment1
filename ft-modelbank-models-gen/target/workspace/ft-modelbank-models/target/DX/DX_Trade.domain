    DX_VARIATION_MARGIN_DETS
        @i18n:Description (en="")
        @t24:i (t24Name=DX.VARIATION.MARGIN.DETS, level=F, type=L, allowedFunctions="L P S", localRefAllowed=false)
    {
        DATE -> byValue DX_Trade:DX_VARIATION_MARGIN_DETS__DATE *
            @t24:i (t24Name=DATE)
        RESERVED3 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=12.0, core=true, t24Name=RESERVED3, mandatory=false)
        RESERVED4 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=13.0, core=true, t24Name=RESERVED4, mandatory=false)
        TRADE_REFERENCE : T24BusinessTypes:N
            @t24:i (align=L, inputBehaviour=N, maxLength=17, core=false, t24Name=TRADE.REFERENCE, genOperation="FIELD(@ID,\".\",1)", mandatory=false)
        /* Updated with value 'RNA' when the reversal trade is authorised. This status is used by COB job DX.POST.VM.INDIV.ENTRIES to post reversal variation margin entries during COB or by online service DX.RV.SERVICE. */
        TRADE_STATUS : T24BusinessTypes:A
            @i18n:Label (en="Trade Status")
            @i18n:Tooltip (en="Status of the transaction in the system")
            @t24:i (align=L, inputBehaviour=N, maxLength=35, sysNumber=11.0, core=true, t24Name=TRADE.STATUS, mandatory=false)
        /* Variation margin ID reference is held against VM.DETS.ID field. ID is of format 'TRADE ID.PORTFOLIO' */
        VM_DETS_ID -> DX_Trade:DX_VARIATION_MARGIN_DETS PK required
            @t24:i (align=L, inputBehaviour=I, maxLength=35, sysNumber=0.0, core=true, businessType=A, t24Name=VM.DETS.ID, mandatory=true)
    }
    DX_VARIATION_MARGIN_DETS__DATE
    {
        /* Holds the account number on which the margin entries are posted. Updated from VAR.ACCOUNT from DX.TRANSACTION record. */
        ACCOUNT -> AC_AccountOpening:ACCOUNT
            @i18n:Label (en=Account)
            @i18n:Tooltip (en="Identifies the customer account")
            @t24:i (align=R, inputBehaviour=N, maxLength=36, sysNumber=2.0, core=true, t24Name=ACCOUNT, mandatory=false)
        /* Valuation amount posted as margin on the date updated in DATE field. Updated from VAR.MARGIN field of DX.TRANSACTION record. */
        AMOUNT : T24BusinessTypes:N
            @i18n:Label (en=Amount)
            @i18n:Tooltip (en="Indicates the Amount")
            @t24:i (align=R, inputBehaviour=N, maxLength=0, sysNumber=5.0, core=true, t24Name=AMOUNT, mandatory=false)
        /* Currency for variation margin. Updated from VAR.MAR.CCY field of DX.TRANSACTION record. */
        CURRENCY -> ST_CurrencyConfig:CURRENCY
            @i18n:Label (en=Ccy)
            @t24:i (align=R, inputBehaviour=N, maxLength=0, sysNumber=3.0, core=true, t24Name=CURRENCY, mandatory=false)
        /* Date on which revaluation had happended. */
        DATE : T24BusinessTypes:D
            @i18n:Label (en=Date)
            @t24:i (align=R, inputBehaviour=N, maxLength=11, sysNumber=1.0, core=true, t24Name=DATE, mandatory=false)
        /* Is the rate of exchange between trade currency and customer account currency. Fields from DATE until EXC.RATE.ACC are associated multivalue fields. Updated from EX.RATE.ACC of DX.TRANSACTION record. */
        EXC_RATE_ACC : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=0, sysNumber=8.0, core=true, t24Name=EXC.RATE.ACC, mandatory=false)
        /* Is the exchange rate between the customer reference currency and trade currency. Updated from EX.RATE.REF of DX.TRANSACTION record. */
        EXC_RATE_REF : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=0, sysNumber=7.0, core=true, t24Name=EXC.RATE.REF, mandatory=false)
        /* Is the rate of exchange between variation margin currency and local currency used for variation margin posting. Updated from VM.EXC.RATE of DX.TRANSACTION record. */
        EXC_RATE_VM : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=0, sysNumber=6.0, core=true, t24Name=EXC.RATE.VM, mandatory=false)
        /* Margin amount in customer reference currency. Updated from VAR.MAR.REF.CCY of DX.TRANSACTION record */
        MARGIN_REF_CCY : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=0, sysNumber=4.0, core=true, t24Name=MARGIN.REF.CCY, mandatory=false)
        RESERVED1 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=9.0, core=true, t24Name=RESERVED1, mandatory=false)
        RESERVED2 : T24BusinessTypes:N
            @t24:i (align=R, inputBehaviour=N, maxLength=35, sysNumber=10.0, core=true, t24Name=RESERVED2, mandatory=false)
    }
